{
  "name": "bs-sedlex",
  "version": "1.99.4-pre.6",
  "description": "An OCaml lexer generator for Unicode",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "@elliottcable/bs-gen": "^0.5.1",
    "@elliottcable/bs-uchar": "^0.0.2",
    "bs-platform": "^4.0.3"
  },
  "devDependencies": {
    "arch": "^2.1.1",
    "archiver": "^2.1.1",
    "cpy": "^7.0.1",
    "del-cli": "^1.1.0",
    "make-dir": "^1.3.0",
    "ppx-sedlex": "=1.99.4-pre.5",
    "node-jq": "^1.3.1"
  },
  "scripts": {
    "env": "env",
    "clean:examples": "cd examples && bsb -clean-world",
    "build:examples": "cd examples && bsb -make-world",
    "clean:runtime": "bsb -clean-world",
    "build:runtime": "bsb -make-world",
    "clean:ppx": "jbuilder clean && del-cli ppx-sedlex/",
    "build:ppx": "jbuilder build",
    "clean": "npm run clean:runtime && npm run clean:ppx",
    "build": "npm run build:runtime && npm run build:ppx",
    "prepare:ppx": "npm run clean:ppx && npm run build:ppx && node scripts/package.js",
    "prepare": "npm run clean:runtime && npm run build:runtime",
    "preversion": "./scripts/preversion.sh",
    "version": "./scripts/version.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "bsconfig.json",
    "sedlex.opam",
    "examples/*",
    "src/*",
    "!**/jbuild",
    "!**/.merlin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ELLIOTTCABLE/bs-sedlex.git"
  },
  "author": "Alain Frisch <alain.frisch@lexifi.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ELLIOTTCABLE/bs-sedlex/issues"
  },
  "homepage": "https://github.com/ELLIOTTCABLE/bs-sedlex#readme"
}
